<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    
    <servlet>
    <servlet-name>SearchServlet</servlet-name>
    <servlet-class>servlet.SearchServlet</servlet-class>
    </servlet>
    
    <servlet>
    <servlet-name>InscriptionServlet</servlet-name>
    <servlet-class>servlet.InscriptionServlet</servlet-class>
    </servlet>
    
    <servlet>
    <servlet-name>AlbumServlet</servlet-name>
    <servlet-class>servlet.AlbumServlet</servlet-class>
    </servlet>
    
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/SearchServlet</url-pattern>
  </servlet-mapping>
  
 <servlet-mapping>
    <servlet-name>InscriptionServlet</servlet-name>
    <url-pattern>/InscriptionServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>AlbumServlet</servlet-name>
  	<url-pattern>/AlbumServlet</url-pattern>
  </servlet-mapping>
 
</web-app>
